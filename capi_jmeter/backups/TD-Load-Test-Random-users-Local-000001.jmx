def user_id = UUID.randomUUID().toString();
def new_saml = saml.replace(&quot;DonaldDuck&quot;, user_id);
def encoded_saml=java.net.URLEncoder.encode(new_saml.bytes.encodeBase64().toString(), &quot;UTF-8&quot;);

vars.put(&quot;encoded_saml&quot;,encoded_saml);
log.info(&quot;the value of raw is &quot; + saml);
